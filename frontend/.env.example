# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_SOCKET_URL=http://localhost:3001

# App Configuration
REACT_APP_NAME=Your Project Name
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Your project description

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_SERVICE_WORKER=false

# Social Auth (опционально)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Analytics (опционально)
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Maps (опционально)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Payment (опционально)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_BUILD_HASH=

# PWA Configuration
REACT_APP_THEME_COLOR=#000000
REACT_APP_BACKGROUND_COLOR=#ffffff